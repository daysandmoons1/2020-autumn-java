import java.util.Scanner;

public class twoDArray {

	public static void main(String[] args) {
		Scanner scanner = new Scanner(System.in);
		int arr[][] = new int[4][4];
		arr = 0;
		
		for(int i=0; i<arr.length; i++);{
			for(int j=0; j<arr[i].length; j++) {
				System.out.print(arr[i][j] + " ");
			}
		}
		scanner.close();
	}

}
